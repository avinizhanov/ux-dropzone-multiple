{
    "name": "yassinehamouten/ux-dropzone-multiple",
    "type": "symfony-bundle",
    "description": "File input dropzones multiple for Symfony Forms",
    "keywords": [
        "symfony-ux"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yassine Hamouten",
            "email": "yassine.hamouten@gmail.com"
        },
        {
            "name": "Titouan Galopin",
            "email": "galopintitouan@gmail.com"
        },
        {
            "name": "Symfony Community",
            "homepage": "https://symfony.com/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Symfony\\UX\\Dropzone\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^4.4.17|^5.0|^6.1",
        "symfony/dependency-injection": "^4.4.17|^5.0|^6.1",
        "symfony/form": "^4.4.17|^5.0|^6.1",
        "symfony/http-kernel": "^4.4.17|^5.0|^6.1"
    },
    "require-dev": {
        "symfony/framework-bundle": "^4.4.17|^5.0|^6.1",
        "symfony/phpunit-bridge": "^5.2",
        "symfony/twig-bundle": "^4.4.17|^5.0|^6.1",
        "symfony/var-dumper": "^4.4.17|^5.0|^6.1"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.3-dev"
        },
        "thanks": {
            "name": "symfony/ux",
            "url": "https://github.com/symfony/ux"
        }
    },
    "minimum-stability": "dev"
}
